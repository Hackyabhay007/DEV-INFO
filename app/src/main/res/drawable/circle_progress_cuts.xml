<!-- circle_progress_cuts.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="4dp"
        android:left="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <shape
            android:innerRadiusRatio="2.5"
            android:shape="ring"
            android:thicknessRatio="15"
            android:useLevel="false">
            <solid android:color="#1F201E" /> <!-- Replace with your desired cut color -->
        </shape>
    </item>
</layer-list>
